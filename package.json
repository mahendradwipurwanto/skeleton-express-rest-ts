{
  "name": "express-ts-typeorm",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node build/src/main.js",
    "dev": "nodemon src/main.ts",
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf coverage build tmp",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "migration:create": "typeorm migration:create src/config/mysql/migrations/$npm_config_name",
    "migration:up": "cross-var typeorm migration:run -d ./build/src/config/postgres/datasource.js",
    "migration:down": "cross-var typeorm migration:revert -d ./build/src/config/postgres/datasource.js",
    "seed:create": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create -n src/config/mysql/seeders/$npm_config_name",
    "seed:up": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./build/src/config/postgres/datasource.js",
    "test": "env-cmd -f .env.test jest --coverage --runInBand"
  },
  "keywords": [],
  "author": "sehat-aqua",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "cross-var": "^1.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "handlebars": "^4.7.8",
    "image-size": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.12.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.16",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.6.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/handlebars": "^4.0.40",
    "@types/jest": "^29.5.13",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  }
}
